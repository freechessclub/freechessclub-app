{
  "name": "freechessclub",
  "version": "2.1.0",
  "license": "MIT",
  "description": "Free Chess Club: A modern web client for FICS.",
  "main": "src/app.js",
  "scripts": {
    "app": "tsc src/app.ts && node app.js && electron src/app.js",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "bundle": "webpack --mode production",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "build": "node app.js && electron-builder -mwl --x64 --arm64 -p always",
    "release": "yarn build",
    "postinstall": "electron-builder install-app-deps && npx patch-package",
    "android": "node app.js && cp -pR assets app/assets && npx cap run android",
    "ios": "node app.js && cp -pR assets app/assets && npx cap run ios"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/freechessclub/freechessclub-app"
  },
  "author": {
    "name": "Free Chess Club Author(s)",
    "email": "support@freechess.club"
  },
  "build": {
    "appId": "club.freechess.FreeChessClub",
    "productName": "Free Chess Club",
    "copyright": "Copyright Â© 2023 Free Chess Club Author(s)",
    "publish": [
      "github"
    ],
    "mac": {
      "category": "public.app-category.board-games"
    },
    "linux": {
      "target": [
        "tar.gz",
        "zip"
      ],
      "category": "Game"
    }
  },
  "dependencies": {
    "@capacitor/android": "next",
    "@capacitor/assets": "^2.0.4",
    "@capacitor/core": "next",
    "@capacitor/ios": "next",
    "autolink-js": "freechessclub/autolink-js",
    "bootstrap": "^5.1.3",
    "cheerio": "^1.0.0-rc.5",
    "chess.js": "^0.12.1",
    "chessground": "^8.3.7",
    "d3": "^7.8.0",
    "electron-updater": "^5.3.0",
    "gh-emoji": "^0.2.5",
    "jquery": "^3.6.4",
    "js-cookie": "^3.0.1",
    "popper.js": "^1.15.0",
    "stockfish.js": "^10.0.2"
  },
  "devDependencies": {
    "@capacitor/cli": "next",
    "@types/bootstrap": "^5.1.7",
    "@types/jquery": "^3.5.1",
    "@types/websocket": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "css-loader": "^6.6.0",
    "electron": "^24.1.3",
    "electron-builder": "^24.3.0",
    "eslint": "^8.30.0",
    "eslint-plugin-jsdoc": "^39.6.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "exports-loader": "^3.1.0",
    "file-loader": "^6.2.0",
    "path-browserify": "^1.0.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "typescript": "^5.0.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.0.2"
  }
}
